{
  "name": "micromark-extension-wikirefs",
  "private": false,
  "version": "0.0.3-mm",
  "description": "Micromark syntax extension for wikirefs (including [[wikilinks]]).",
  "license": "GPL-3.0",
  "author": {
    "name": "manunamz",
    "email": "manuanmz@pm.me"
  },
